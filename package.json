{
  "name": "vite-plugin-clt-svr",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.5",
  "description": "Allow communication between local services and clients",
  "author": "liuseen-l",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vite-plugins/vite-plugin-clt-svr"
  },
  "bugs": {
    "url": "https://github.com/vite-plugins/vite-plugin-clt-svr/issues"
  },
  "keywords": [
    "vite-plugin",
    "client",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "nr clear && rollup -c rollup.config.ts --configPlugin typescript",
    "clear":"rimraf dist",
    "lint": "eslint .",
    "play": "nr -C playground dev",
    "prepare": "husky install",
    "release": "bumpp && nr changelog && npm publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "peerDependencies": {
    "vite": "^3.0.0-0 || ^4.0.0-0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@antfu/ni": "^0.21.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.3.3",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
