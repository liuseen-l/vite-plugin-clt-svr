{
  "name": "vite-plugin-clt-svr",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.5",
  "author": "liuseen-l",
  "license": "MIT",
  "keywords": [
    "vite-plugin",
    "client",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
